{
  "source": {
    "include": [
      "./src/",
      "./README.md"
    ],
    "includePattern": "\\.(js)$",
    "exclude": [
      "./node_modules/",
      "./dist/",
      "./build/"
    ]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "metadata": {
    "title": "CTrace GUI Documentation",
    "description": "Complete documentation for the CTrace GUI Electron application"
  }
}